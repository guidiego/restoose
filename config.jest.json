{
    "roots": [
      "tests"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
        "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "moduleNameMapper": {
      ".*config.jest.json": "<rootDir>/tests/__mocks__/config.mock.json"
    }
}
