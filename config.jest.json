{
    "roots": [
        "restoose"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
        "/node_modules/"
    ],
    "collectCoverageFrom": [
      "restoose/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/restoose/utils/index.js",
      "<rootDir>/restoose/logger/index.js"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ]
}
