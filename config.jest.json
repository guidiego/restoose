{
    "roots": [
        "restoose"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
        "/node_modules/"
    ],
    "collectCoverageFrom": [
      "restoose/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/restoose/utils/index.js",
      "<rootDir>/restoose/logger/index.js",
      "<rootDir>/restoose/connector/index.js"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      },
      "each": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
}
